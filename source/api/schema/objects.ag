guard ActorBase: {
	"actor_id": string,
	"name": string
};

guard Actor: ActorBase & {};

guard ArtistBase: {
	"artist_id": string,
	"title": string
};

guard Artist: ArtistBase & {
	"albums": Album[]
};

guard AlbumBase: {
	"album_id": string,
	"title": string,
	"artwork": ../../database/schema/ImageFile[]
};

guard Album: AlbumBase & {
	"artists": ArtistBase[],
	"discs": Disc[],
	"year"?: number
};

guard DiscBase: {
	"disc_id": string,
	"album": AlbumBase,
	"number": number
};

guard Disc: DiscBase & {
	"tracks": Track[]
};

guard TrackBase: {
	"track_id": string,
	"title": string,
	"disc": DiscBase,
	"number": number
};

guard Track: TrackBase & {
	"artists": ArtistBase[],
	"last_stream_date"?: number,
	"media": ../../database/schema/AudioFile,
	"copyright"?: string
};

guard UserBase: {
	"user_id": string,
	"name": string,
	"username": string
};

guard User: UserBase & {};

guard PlaylistBase: {
	"playlist_id": string,
	"title": string,
	"description": string,
	"user": UserBase
};

guard Playlist: PlaylistBase & {
	"items": PlaylistItem[]
};

guard PlaylistItemBase: {
	"playlist_item_id": string,
	"number": number,
	"playlist": PlaylistBase,
	"track": Track
};

guard PlaylistItem: PlaylistItemBase & {};

guard GenreBase: {
	"genre_id": string,
	"title": string
};

guard Genre: GenreBase & {};

guard MovieBase: {
	"movie_id": string,
	"title": string,
	"artwork": ../../database/schema/ImageFile[]
};

guard Movie: MovieBase & {
	"year"?: number,
	"summary"?: string,
	"genres": Genre[],
	"actors": Actor[],
	"last_stream_date"?: number,
	"media": ../../database/schema/VideoFile,
	"subtitles": ../../database/schema/SubtitleFile[],
	"copyright"?: string
};

guard ShowBase: {
	"show_id": string,
	"title": string,
	"artwork": ../../database/schema/ImageFile[]
};

guard Show: ShowBase & {
	"summary"?: string,
	"genres": Genre[],
	"actors": Actor[],
	"seasons": Season[]
};

guard SeasonBase: {
	"season_id": string,
	"number": number,
	"show": ShowBase
};

guard Season: SeasonBase & {
	"episodes": Episode[]
};

guard EpisodeBase: {
	"episode_id": string,
	"title": string,
	"number": number,
	"season": SeasonBase
};

guard Episode: EpisodeBase & {
	"year"?: number,
	"summary"?: string,
	"last_stream_date"?: number,
	"media": ../../database/schema/VideoFile,
	"subtitles": ../../database/schema/SubtitleFile[],
	"copyright"?: string
};

guard SubtitleBase: {
	"subtitle_id": string,
	"subtitle": ../../database/schema/SubtitleFile
};

guard Subtitle: SubtitleBase & {
	"cues": Cue[]
};

guard CueBase: {
	"cue_id": string,
	"subtitle": SubtitleBase,
	"start_ms": number,
	"duration_ms": number,
	"lines": string[]
};

guard Cue: CueBase & {
	"media": Episode | Movie
};

guard YearBase: {
	"year_id": string,
	"year": number
};

guard Year: YearBase & {};

guard EntityBase: ActorBase | AlbumBase | ArtistBase | CueBase | DiscBase | EpisodeBase | GenreBase | MovieBase | PlaylistBase | SeasonBase | ShowBase | TrackBase | UserBase | YearBase;

guard Entity: Actor | Album | Artist | Cue | Disc | Episode | Genre | Movie | Playlist | Season | Show | Track | User | Year;
